/*=========== Basic style ==========*/
body{
    margin: 0;

    @include mobile-max{
          margin-top: -4%;
    }
    
}

div{
    display: block;
}

input{
    border: none;
    padding: 0;
    font-family: Ubuntu,sans-serif;
    font-weight: $font-light;
}

input[type=submit]{
    cursor: pointer;
}

ul{
    list-style: none;
    padding: 0;
    margin: 0;
}

a{
    text-decoration: none;
    cursor: pointer;
    color: #6D8FA0;
}

.hide{
  display: none!important;
}

.relative{
    position: relative;
 
}

.absolute{
    position: absolute;
}

.no-repeat{
    background-repeat: no-repeat;
}

.bg-cover{
    background-size: cover;
}

.border-box{
    box-sizing: border-box;
}

.d-flex{
    display: flex;
}

.flex-wrap{
  flex-wrap: wrap;
}

.space-between{
    justify-content: space-between;
    @include tablet-max{
      flex-direction: column;
  }
    @include tablet-max-higher{
      flex-direction: column;
  }

}

.space-around{
    justify-content: space-around;
}

.align-center{
    align-items: center;
}

.container-lg{
    max-width: 1440px;
}

.center-block{
    margin: 0 auto;
}

.small-margin{
    margin-left: 10px;
    margin-right: 10px;
}



.large-margin{
    margin-left: 150px;
    margin-right: 150px;

  
    @include mobile-max{
        margin-left: 10px!important;
        margin-right: 10px!important;
    }

    @include desktop-semi-sm{
      margin-left: 80px!important;
      margin-right: 80px!important;
    }

    @include laptop-sm{
      margin-left: 10px !important;
      margin-right: 10px !important;
    }
  
    @include tablet-max{
      margin-left: 0;
      margin-right: 0;
    }


}

.xlarge-margin{
  margin-left: 300px;
  margin-right: 300px;

  @include mobile-max{
    margin-left: 30px;
    margin-right: 30px;
  }
}

img{
    display: block;
}

.mobile-only{
    display: none;

    @include mobile-max{
        display: block;
    }

    @include tablet-max{
      display: block;
    }
}

.desktop-only{

    @include mobile-max{
        display: none;
    }

    @include tablet-max{
      display: none;
    }
}

.blue-star{
    margin-right: 5px;
}

#MainContent{
  position: relative;
  z-index: 1;

  @include mobile-max{
    padding-top: 13%;
  }
  @include laptop-sm{
      overflow-x: hidden;
    }
    @include tablet-max-higher{
      overflow-x: hidden;
    }
    @include tablet-max{
      overflow-x: hidden;
    }
    @include tablet-max-new{
      overflow-x: hidden;
    }
    @include mobile-max-smaller{
      overflow-x: hidden;
    }
    @include mobile-max {
      overflow-x: hidden;
    }
    @include mobile-max-xm-xll {
      overflow-x: hidden;
    }
}

/*============ Button ==========*/
button{
display: block;
border: none;
cursor: pointer;
}

button:focus{
  outline: 0;
}

.pbutton{
width: 166px;
height: 50px;
font-weight: $font-bold;
font-size: 15px;
color: white;
line-height: 16px;
padding: 18px 0;

  &:hover{
    background: $linear-green;
  }
}

/*================ #Helper Classes ================*/

// Only show when JS is not supported
.no-js:not(html) {
  display: none;

  .no-js & {
    display: block;
  }
}

// Only show when JS is supported
.js {
  .no-js & {
    display: none;
  }
}

.hide {
  display: none !important;
}


