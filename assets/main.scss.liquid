  width: 100%!important;
}

#searchMagnifier{
  position: absolute;
  top: 32%;
  left: 1%;
  width: 14px;
  height: 14px;

  @include mobile-max{
    left: 3%;
  }
}

#searchInput{
    width: 95%;
    height: 46px;
  
  	background: #FFFFFF;
    opacity: 0.5;
    border: 1px solid #A7A7A7;
    box-sizing: border-box;
    box-shadow: 0px 1px 10px rgba(0, 0, 0, 0.1);
    border-radius: 5px;
