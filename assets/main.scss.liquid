/*========== Search autocomplete ==========*/
.search-results {
  z-index: 8889;
  list-style-type: none;
  max-width: 128%;
  width: 1440px;
  height: 100vh;
  box-sizing: border-box;
  padding: 46px 0 194px 150px;
  background: white;
  overflow-y: scroll;

  @include mobile-max{
    width: 106%;
    top: 49px!important;
    left: -10px!important;
    padding: 26px 0 0 22px;
  }
}
.search-results li {
  display: block;
  width: 100%;
  height: 38px;
  margin-bottom: 30px;
  line-height: 38px;
  overflow: hidden;

  @include mobile-max{
    margin-bottom: 6px;
  }
}
.search-results li:first-child {
  border-top: none;
}
.search-results .title {
  float: left;
  font-size: 30px;
  font-weight: 300;
  white-space: nowrap;
  overflow: hidden;
  /* The text-overflow property is supported in all major browsers. */  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  text-align: left;

  @include mobile-max{
    font-size: 16px;
  }
}


/*========= Collection filter ==========*/
.active-filter{
  background: #7e9da3!important;
}

.filter-title{
  width: 100%;
  padding: 21px 29px;
  box-sizing: border-box;
  cursor: pointer;
  background: #f2f5f7;
}

#filter-system{
  border-top: 0.5px solid rgba(255,255,255,0.95);

      li{
        text-align: center;
        color: #202020;
        flex: 0 0 50%;
        padding: 18px 0;
        background: rgba(255,255,255,0.2);
        border-left: 0.5px solid rgba(255,255,255,0.95);
      }
  }

  .extra-filter{
    text-align: center;
    color: #202020;
    flex: 0 0 50%;
    padding: 18px 0;
    background: rgba(255,255,255,0.2);
    border-left: 0.5px solid rgba(255,255,255,0.95);
    box-sizing: border-box;
    width: calc(100%/2);
    max-width: 100%;
    height: 54px;
    bottom: 0;
    right: 0;
  }

.filter-content{
  line-height: 45px;
  padding: 40px 0 38px;
}

.filter-title:hover{
  background: #e7edef;
}

#filter-system > li.tagsort-active{
  background: rgba(255,255,255,0.4);
}

.tagsort-active{
  color: #042338;
  font-weight: $font-bold;
}

.tag-container{
  li{
    text-transform: capitalize;
    cursor: pointer;
  }
}

/*========== Mobile category filter / Mobile more menu ==========*/
.mobile-filter, .mobile-more-menu{
  transform: translateY(100%);
  opacity: 0;
  z-index: 2;
  position: fixed;
  top: 160px;
  left: 0;
  overflow-y: scroll;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 40px 10% 40px;
  transition: translateY .4s ease-in-out;
}

.mobile-more-menu{
  z-index: 2;
  padding: 40px 10% 55%;
}

.mobile-filter-remove, .mobile-more-remove{
  top: 3%;
  right: 8%;
  cursor: pointer;
}

