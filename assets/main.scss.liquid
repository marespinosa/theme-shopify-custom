@charset "UTF-8";
/*========== Mixins ==========*/

@mixin mobile-max-xm-xll{
  @media (max-width: 290px) { @content; }
}

@mixin mobile-max{
    @media (max-width: 425px) { @content; }
}

@mixin mobile-max-smaller{
  @media (max-width: 490px) { @content; }
}


@mixin tablet-max-higher{
    @media (max-width: 920px) { @content; }
  }
@mixin tablet-max{
    @media (max-width: 768px) { @content; }
  }

@mixin tablet-max-new{
    @media (max-width: 600px) { @content; }
  }


@mixin desktop-semi-sm{
    @media ( max-width: 1290px ) { @content; }
}


@mixin laptop-sm{
    @media ( max-width: 1024px ) { @content; }
}

@mixin laptop-sm-90{
    @media ( max-width: 1090px ) { @content; }
}

@mixin laptop-rg{
    @media ( max-width: 1440px ) { @content; }
}

@mixin laptop-lg{
    @media ( min-width: 1920px ) { @content; }
}

@mixin laptop-mg{
    @media ( min-width: 2560px ) { @content; }
}


