<script type="text/javascript">
    let waitJQueryInit = setInterval(function() {
        if (typeof $ === "function") {
            clearInterval(waitJQueryInit);

            $('.tabs .tab-link').on('click', function(e) {
                $('.tabs .tab-link').removeClass('current');
                $(e.currentTarget).addClass('current');
                let target = $(e.currentTarget).attr('data-target');
                $('.tab-content').removeClass('current');
                $(`.tab-content[data-target="${target}"]`).addClass('current');
            });
          
			$('.accordions-system-faqs .system-accordion-win h5').on('click', function(e) {
              $('.text-system-accordion-win').slideToggle();                                             
            });
          
          	$('.accordions-system-faqs .faqs-accordion-win h5').on('click', function(e) {
              $('.text-faq-accordion-win').slideToggle();                                             
            });
          
            $('.choose-os ul li').unbind('click').on('click', function(e) {
                $('.choose-os ul li').removeClass('current');
                let target = $(e.currentTarget).attr('data-target');
                $('.pc-prices').removeClass('current');
                $('.mac-prices').removeClass('current');
                if (target == 'pc') {
                    $('.pc-prices').addClass('current');
                    $(`.choose-os ul li[data-target="${target}"]`).addClass('current');
                }
                if (target == 'mac') {
                    $('.mac-prices').addClass('current');
                    $(`.choose-os ul li[data-target="${target}"]`).addClass('current');
                }
            });
        }
    }, 300);
</script>